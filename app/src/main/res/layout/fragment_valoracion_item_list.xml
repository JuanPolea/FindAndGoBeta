<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.findandgoapp.fragment.ValoracionFragment"
    android:orientation="vertical"
    android:name="@string/fragment_valoracion_item_list">


    <ListView
        android:id="@+id/valoracionItemList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        android:divider="@android:color/transparent"
        android:dividerHeight="@dimen/size_5"


        >

    </ListView>

    <com.findandgoapp.custom.CustomFontTextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvTituloNotificaciones"
        app:font="@string/font_name_source_amatic_regular_bold" />
</LinearLayout>