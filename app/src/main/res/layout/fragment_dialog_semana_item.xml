<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/AlertDialog.AppCompat.Light"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@color/blancoTpte"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/idChexbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:background="@drawable/btn_check"
        android:paddingLeft="@dimen/size_5"
        android:text="@string/sLunes"
        tools:ignore="ContentDescription" />

    <com.findandgoapp.custom.CustomFontTextView
        android:id="@+id/idTVDia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/size_10"
        android:layout_toRightOf="@id/idChexbox"
        android:text=""
        app:font="@string/font_name_source_amatic_regular" />


</RelativeLayout>