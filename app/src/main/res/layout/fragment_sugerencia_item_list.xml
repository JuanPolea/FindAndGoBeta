<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_sugerencia_item"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    android:layout_alignParentStart="@dimen/margen_izq"
    android:layout_marginBottom="@dimen/size_20"
    android:layout_marginEnd="@dimen/margen_dcho"
    android:layout_marginTop="?attr/actionBarSize">


    <com.findandgoapp.custom.CustomFontTextView
        android:id="@+id/idTvSugerencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:focusable="false"
        android:focusableInTouchMode="false"

        android:gravity="left"
        android:textSize="@dimen/tamano_fuente_menu"
        app:font="@string/font_name_source_amatic_regular"
        tools:ignore="RelativeOverlap" />

    <com.findandgoapp.custom.CustomFontTextView
        android:id="@+id/idTvSugerenciaCont"
        android:layout_width="@dimen/size_circulitos"
        android:layout_height="@dimen/size_circulitos"
        android:layout_alignBottom="@+id/idTvSugerencia"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="16dp"
        android:background="@drawable/borde_rojo"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="center_vertical|center_horizontal"
        android:text="@string/iCero"
        android:textColor="@color/white"
        android:textSize="@dimen/tamano_letra"
        app:font="@string/font_name_source_amatic_regular" />

</RelativeLayout>