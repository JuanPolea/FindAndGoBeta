<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/fondo"
    tools:context="com.findandgoapp.fragment.ValoracionFragment">

    <com.findandgoapp.custom.CustomFontTextView
        android:id="@+id/tvTituloNotificaciones"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/size_5"
        android:layout_marginTop="@dimen/size_30"
        android:paddingTop="@dimen/size_2"
        android:text="@string/sTituloNotificaciones"
        android:textColor="@color/negro"
        android:textSize="@dimen/size_medium"
        android:visibility="visible"
        app:font="@string/font_name_source_amatic_regular_bold" />

    <ListView
        android:layout_below="@id/tvTituloNotificaciones"
        android:id="@+id/listNotificaciones"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        android:divider="@android:color/transparent"
        android:dividerHeight="@dimen/size_5"

        >

    </ListView>

</RelativeLayout>