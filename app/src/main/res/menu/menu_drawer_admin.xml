<menu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <group android:checkableBehavior="single">

        <item android:title="@string/sEventos">
            <menu>
                <item xmlns:app="http://schemas.android.com/apk/res-auto"

                    android:id="@+id/miIdBuscarPorDatos"
                    android:checkable="false"
                    android:icon="@drawable/ic_action_search"
                    android:title="@string/sBuscarPorDatos"
                    app:showAsAction="ifRoom" />
                <item xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/miIdCrearEvento"
                    android:checkable="false"
                    android:icon="@drawable/ic_action_new_event"
                    android:title="@string/sCrearEvento"
                    app:showAsAction="never" />
                <item
                    android:id="@+id/miIdMisEventos"

                    android:checkable="false"
                    android:icon="@drawable/ic_action_mis_eventos"
                    android:title="@string/sMisEventos"
                    app:showAsAction="never" />
                <item xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/miIdActualizarEventos"
                    android:checkable="false"
                    android:icon="@drawable/home"
                    android:title="@string/home"
                    app:showAsAction="never" />
            </menu>
        </item>
    </group>


</menu>
