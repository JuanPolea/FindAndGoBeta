<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_sancion_recibida"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/blanco"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    tools:context="com.findandgoapp.adapter.ListAdapterValoracion"
    tools:showIn="@layout/activity_sancion_recibida">


    <com.findandgoapp.custom.CustomFontTextView
        android:id="@+id/tvSancion"
        style="@style/Texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/iCero"
        android:textColor="@color/negro"
        app:font="@string/font_name_source_amatic_regular"

        />


</RelativeLayout>
