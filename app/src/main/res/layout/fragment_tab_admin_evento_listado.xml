<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragment_tab_admin_evento_listado"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.findandgoapp.fragment.TabAdminEventoListado">


    <com.findandgoapp.custom.CustomFontTextView
        android:id="@+id/tvTabAdminEventoListadoNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:font="@string/font_name_source_amatic_regular"
        android:textSize="@dimen/size_20"
        android:textColor="@color/link"
        tools:ignore="RelativeOverlap" />

    <ImageView
        android:id="@+id/ivNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_arrow"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        tools:ignore="ContentDescription" />

</RelativeLayout>
